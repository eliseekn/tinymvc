namespace NAMESPACE;

use App\Database\Models\MODELNAME;

class CLASSNAME
{
    public function handle(int $id)
	{
        $MODELNAME = MODELNAME::find($id);

        if ($MODELNAME === false) {
            return false;
        }

        return $MODELNAME->delete();
	}
}
